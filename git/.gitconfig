[user]
	name = Jonathan Chow
	email = me@echo.sh
[push]
	default = simple
[alias]
	l = log --graph --pretty=format:'%C(yellow)%h%Cred%d %C(cyan)%an %Creset%s %Cgreen(%cr)'
	o = checkout
	p = pull --rebase -p
	s = status
	c = commit
	n = clone
	e = cherry-pick
	d = diff
	w = diff --word-diff=color
	t = stash
	g = gui
	u = push
	f = fetch -p
	m = merge --no-ff
	ff = merge --ff-only
	a = add
	r = rebase
	rc = rebase --continue
	mt = mergetool
	b = branch
	rh = reset --hard
[color]
	ui = true
[core]
	editor = vim
[merge]
	defaultToUpstream = true
	tool = vimdiff
[mergetool]
	keepBackup = false
